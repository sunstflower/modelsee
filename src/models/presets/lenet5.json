{
  "id": "lenet5",
  "name": "LeNet-5",
  "description": "Classic convolutional neural network for handwritten digit recognition",
  "framework": "tensorflow",
  "category": "cnn",
  "complexity": "simple",
  "input_spec": {
    "dataset": "mnist",
    "shape": [28, 28, 1]
  },
  "layers": [
    {
      "type": "conv2d",
      "config": {
        "filters": 6,
        "kernel_size": [5, 5],
        "strides": [1, 1],
        "padding": "same",
        "activation": "tanh",
        "name": "C1"
      }
    },
    {
      "type": "avgPooling2d",
      "config": {
        "pool_size": [2, 2],
        "strides": [2, 2],
        "name": "S2"
      }
    },
    {
      "type": "conv2d",
      "config": {
        "filters": 16,
        "kernel_size": [5, 5],
        "strides": [1, 1],
        "padding": "valid",
        "activation": "tanh",
        "name": "C3"
      }
    },
    {
      "type": "avgPooling2d",
      "config": {
        "pool_size": [2, 2],
        "strides": [2, 2],
        "name": "S4"
      }
    },
    {
      "type": "flatten",
      "config": {
        "name": "flatten"
      }
    },
    {
      "type": "dense",
      "config": {
        "units": 120,
        "activation": "tanh",
        "name": "C5"
      }
    },
    {
      "type": "dense",
      "config": {
        "units": 84,
        "activation": "tanh",
        "name": "F6"
      }
    },
    {
      "type": "dense",
      "config": {
        "units": 10,
        "activation": "softmax",
        "name": "OUTPUT"
      }
    }
  ]
}
